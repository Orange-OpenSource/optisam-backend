{
   "swagger": "2.0",
   "info": {
      "title": "import service",
      "version": "version not set"
   },
   "schemes": [
      "http",
      "https"
   ],
   "paths": {
      "/api/v1/import/download": {
         "post": {
            "summary": "download report/error/source file.",
            "consumes": [
               "multipart/form-data"
            ],
            "parameters": [
               {
                  "in": "formData",
                  "name": "downloadType",
                  "type": "string",
                  "required": true,
                  "description": "values would be one of these value error/analysis/source"
               },
               {
                  "in": "formData",
                  "name": "uploadId",
                  "type": "string",
                  "required": true,
                  "description": "This field is required when downloadType is error/source"
               },
               {
                  "in": "formData",
                  "name": "scope",
                  "type": "string",
                  "required": true
               },
               {
                  "in": "formData",
                  "name": "fileName",
                  "type": "string",
                  "required": true,
                  "description": "This field is required when downloadType is analysis"
               }
            ],
            "responses": {
               "200": {
                  "description": "A successful response."
               }
            }
         }
      },
      "/api/v1/import/upload": {
         "post": {
            "summary": "Upload file or files.",
            "consumes": [
               "multipart/form-data"
            ],
            "parameters": [
               {
                  "in": "formData",
                  "name": "file",
                  "type": "file",
                  "required": true
               },
               {
                  "in": "formData",
                  "name": "scope",
                  "type": "string",
                  "required": true
               },
               {
                  "in": "formData",
                  "name": "uploadType",
                  "type": "string",
                  "required": true
               }
            ],
            "responses": {
               "200": {
                  "description": "A successful response."
               }
            }
         }
      },
